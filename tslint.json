{
  "extends": "tslint:recommended",
  "rules": {
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "comment-format": [true, "check-lowercase", {"ignore-words": ["TODO"]}],
    "no-var-requires": false,
    "typedef": [true, "call-signature", "parameter", "member-variable-declaration", "arrow-call-signature", "arrow-parameter"],
    "no-empty-interface": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "object-literal-key-quotes": [true, "as-needed"],
    "max-line-length": [false],
    "no-console": [false, "log"],
    "no-trailing-whitespace": [true, "ignore-blank-lines", "ignore-comments"],
    "object-literal-sort-keys": false,
    "no-unused-variable": [true],
    "no-consecutive-blank-lines": false
  },
  "jsRules": {

  }
}